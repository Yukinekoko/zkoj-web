<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="indi.snowmeow.zkojweb.mapper.SolutionSourceCodeMapper">

    <insert id="insert" parameterType="SolutionSourceCode">
        insert into solution_source_code(solution_id, source_code)
        values (#{solutionId},
                #{sourceCode})
    </insert>

</mapper>