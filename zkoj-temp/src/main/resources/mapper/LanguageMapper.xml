<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="indi.snowmeow.zkojweb.mapper.LanguageMapper">

    <select id="findById" resultType="LanguagePO">
        select * from language where id=#{id} limit 1
    </select>
    <select id="list" resultType="LanguagePO">
        select id, name from language
    </select>
</mapper>